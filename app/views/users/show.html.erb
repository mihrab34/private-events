<div>
  <h1><%= @user.name.capitalize %>'s Events</h1>
  <i class="fa fa-calender" aria-hidden="true"></i></span>
    <h3>Created Events</h3>
    <ul>
      <% @user.events.each do |event| %>
        <li>
          <%= event.title.capitalize %><br>
          <%= event.description %><br>
          <%=event.date %><br>
          <%= link_to 'Show', event %>
      </li> 
      <% end %>
    </ul>

    <div>
    <i class="fa fa-calender" aria-hidden="true"></i></span>
    <h3>Upcoming Events</h3>
    <ul>
      <% @upcoming_events.each do |event| %>
        <li>
          <%= event.title.capitalize %><br>
          <%= event.description %><br>
          <%=event.date %><br>
          <%= link_to 'Show', event %>
      </li>
      <% end %>
    </ul>
    </div>  
    <div>
    <i class="fa fa-calender" aria-hidden="true"></i></span>
    <h3>Previous Events</h3>
    <ul>
      <% @prev_events.each do |event| %>
        <li>
          <%= event.title %><br>
          <%= event.description %><br>
          <%=event.date %><br>
          <%= link_to 'Show', event %>
      </li>
      <% end %>
    </ul>
    </div>
</div>
